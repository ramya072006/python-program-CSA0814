def binary_addition(bin1, bin2):
   
    num1 = int(bin1, 2)
    num2 = int(bin2, 2)
    
    sum_result = num1 + num2
    
    binary_sum = bin(sum_result)[2:] 
    
    return binary_sum

bin1 = "1101"
bin2 = "1011"

result = binary_addition(bin1, bin2)
print(f"The sum of {bin1} and {bin2} is: {result}")
